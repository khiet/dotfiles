[core]
	editor = vim
[color]
	ui = true
[alias]
	st = status
	co = checkout
	br = branch
	aa = add -A
  ci = commit -av
  ft = fetch
  ds = diff --staged
  lg = log --pretty=format:'%C(yellow)%h%Creset %s %Cgreen(%cd) %C(red)%an%Creset'
  lgg = log --graph --decorate --oneline
  dm = diff origin/master
  rmc = rm -r --cached .
[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
[user]
	name = Khiet Le
	email = khiet.le@ehochef.com
[pull]
	default = current
[push]
	default = current
