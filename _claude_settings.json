{
  "statusLine": {
    "type": "command",
    "command": "input=$(cat); printf '\\033[38;5;208m%s\\033[0m in \\033[38;5;39m%s\\033[0m' \"$(echo \"$input\" | jq -r '.model.display_name')\" \"$(basename \"$(echo \"$input\" | jq -r '.workspace.current_dir')\")\""
